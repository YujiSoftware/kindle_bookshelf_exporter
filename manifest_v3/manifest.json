{
  "name": "Kindle bookshelf exporter",
  "version": "0.9.2",
  "manifest_version": 3,
  "description": "Export your Kindle book list.",
  "applications": {
    "gecko": {
      "id": "@kindle-bookshelf-exporter"
    }
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "https://www.amazon.co.jp/hz/mycd/digital-console/contentlist/booksAll/*"
  ],
  "icons": {
    "48": "icon@48.png",
    "96": "icon@96.png"
  },
  "action": {
    "default_icon": {
      "32": "icon@32.png",
      "48": "icon@48.png",
      "96": "icon@96.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}